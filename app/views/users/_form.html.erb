<%= form_with model: @user do |form| %>
  <%= form.label :username %>
  <%= form.text_field :username, placeholder: "John or Jane Doe" %><br><br>
  <%= form.label :email %>
  <%= form.text_field :email, placeholder: "example@example.com" %><br><br>
  <%= form.label :password %>
  <%= form.text_field :password, placeholder: "1234" %><br><br>
  <%= form.submit "Submit" %>
<% end %>   